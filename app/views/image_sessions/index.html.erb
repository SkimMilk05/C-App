<p id="notice"><%= notice %></p>

<h1>Image Sessions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_sessions.each do |image_session| %>
      <tr>
        <td><%= link_to 'Show', image_session %></td>
        <td><%= link_to 'Edit', edit_image_session_path(image_session) %></td>
        <td><%= link_to 'Destroy', image_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image Session', new_image_session_path %>
